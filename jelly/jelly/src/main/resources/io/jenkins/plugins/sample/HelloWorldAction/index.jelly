<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout" xmlns:st="jelly:stapler">
    <l:layout title="Greeting">
        <l:main-panel>
            <!-- Escaped by default -->
            <h1>Name: ${it.name}</h1>

            <?jelly escape-by-default='false'?>
            <!-- Explicitly unescaped -->
            <h1>Unescaped Name: ${it.name}</h1>



        </l:main-panel>
    </l:layout>
</j:jelly>